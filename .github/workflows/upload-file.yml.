name: Upload File

on:
  push:
    branches:
      - main

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout code
        uses: actions/checkout@v2

      - name: Upload file
        uses: actions/upload-artifact@v2
        with:
          name: my-file
          path: C:\Users\lfinc\Dropbox\Results Screen\Backup Results Files\Results.html
